// Example program
#include <iostream>
#include <string>
#include<cmath>
using namespace std;

long long check(long long middle, long long n)
{
    //cout<<"in check()"<<endl;
    long long i, sum=0;
    for(i=middle; i>0; i--)
    {
        sum += pow(i, 3);
    }
    cout<<"in check()"<<" middle = "<<middle<<" sum = "<<sum<<endl;
    return sum;
}

long long qs(long long m)
{
    long long first, p1, middle, p2, last, i, temp; 
    long long n = m/1000;
    first = 1;
    last = n;
    while(first<=last)
    {
        middle  = (first+last)/2;
        p1 = first + (last-first)/4;
        p2 = first + (last-first)*(3/4);
        
        temp = check(middle, n);
        
        if(temp == m) return middle;
        else if(temp < m) first = middle;
        else if(temp > m) last = middle;
    }
}

long long bs(long long m)
{
    cout<<"in bs()"<<endl;
    long long first, last, middle, i, temp; 
    long long n = m/1000;
    first = 1;
    last = n;
    cout<<"in bs()"<<" m = "<<m<<" n = "<<n<<endl;
    while(first<=last)
    {
        middle  = (first+last)/2;
        temp = check(middle, n);
        if(temp == m) return middle;
        else if(temp < m) first = middle;
        else if(temp > m) last = middle;
    }
    if(first>last) return -1;
}


